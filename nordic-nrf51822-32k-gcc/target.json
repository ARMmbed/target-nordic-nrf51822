{
  "name": "nordic-nrf51822-32k-gcc",
  "version": "0.0.7",
  "description": "Official mbed build target for the nrf51822 32KB chip.",
  "homepage": "https://github.com/ARMmbed/target-nrf51822",
  "repository": {
    "url": "git@github.com:ARMmbed/target-nrf51822.git",
    "type": "git"
  },
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    },
    {
      "type": "LicenseRef-BSD-3clause-Nordic.txt"
    },
    {
      "type": "LicenseRef-s130_nrf51822_1.0.0_licence_agreement.txt"
    }
  ],
  "inherits": {
    "mbed-gcc": "*"
  },
  "keywords": [
    "mbed-target:nrf51822",
    "mbed-official",
    "nrf51822",
    "nordic",
    "gcc"
  ],
  "config": {
    "mbed-os": true,
    "minar": {
      "initial_event_pool_size": 4,
      "additional_event_pools_size": 4
    },
    "chip": {
      "nrf51822": {
        "32k": true
      }
    }
  },
  "similarTo": [
    "nrf51822",
    "nrf",
    "nordic",
    "cortex-m0",
    "armv6-m"
  ],
  "toolchain": "CMake/toolchain.cmake",
  "private" : true,
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "NRF51822",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "NRF51822",
      "$program"
    ]
  }
}

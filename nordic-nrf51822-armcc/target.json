{
  "name": "nordic-nrf51822-armcc",
  "version": "1.0.0",
  "description": "Official mbed build target for the mbed nrf51822 development board, using the armcc toolchain.",
  "homepage": "https://github.com/ARMmbed/target-nordic-nrf51822",
  "repository": {
    "url": "git@github.com:ARMmbed/target-nordic-nrf51822.git",
    "type": "git"
  },
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    },
    {
      "type": "LicenseRef-BSD-3clause-Nordic.txt"
    },
    {
      "type": "LicenseRef-softdevice_nrf51822_licence_agreement.txt"
    }
  ],
  "inherits": {
    "mbed-armcc": "*"
  },
  "keywords": [
    "mbed-target:nrf51822",
    "mbed-official",
    "nrf51822",
    "nordic",
    "armcc"
  ],
  "config": {
    "mbed-os": {
      "stdio": {
        "default_baud": 9600
      }
    },
    "minar": {
      "initial_event_pool_size": 4,
      "additional_event_pools_size": 4
    },
    "image": {
      "fota": false,
      "heap": {
        "warning_threshold": 1024
      }
    },
    "hardware": {
      "clocks": {
        "nrf": {
          "lfclk_src": 1
        }
      }
    },
    "cmsis": {
      "nvic": {
        "has_custom_vtor": true
      }
    }
  },
  "similarTo": [
    "nrf51822",
    "nrf",
    "nordic",
    "cortex-m0",
    "armv6-m"
  ],
  "toolchain": "CMake/toolchain.cmake",
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "NRF51822",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "NRF51822",
      "$program"
    ]
  }
}
